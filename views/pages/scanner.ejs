        var apiData = $('form').serializeArray().reduce(function (obj, item) {
            obj[item.name] = item.value;
            return obj;
        }, {});
        if (apiData.firstName == '') {
            return alert('First name required!');
        }
        if (apiData.lastName == '') {
            return alert('Last name required!');
        }
        if (apiData.email == '' || !(/^[\-0-9a-zA-Z\.\+_]+@[\-0-9a-zA-Z\.\+_]+\.[a-zA-Z]{2,}$/).test(apiData.email)) {
            return alert('Email is not valid.');
        }
        $.ajax({
            type: "POST",
            url: '/submit-form',
            data: apiData,
            dataType: "json",
            // processData: false,
            // cache : false,
            success: function (res, err) {
                if (res.status) {
                    $(document).find('.step3').hide();
                    $(document).find('.step4').show();
                    setTimeout(function () {
                        location.reload();
                    }, 5000);
                } else {
                    alert("Unable to submit form");
                }
            }
        })
    })
